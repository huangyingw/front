[remote "Minds"]
    url = https://gitlab.com/minds/front.git 
    fetch = +refs/heads/*:refs/remotes/Minds/*
[push]
    remote = origin
[gsync]
    remote = Minds
    branch = master
[commit]
    message = n
[remote "origin"]
	url = git@github.com:huangyingw/front.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[gdif]
    ignore = install.sh
    ignore = run.sh
    ignore = serve.sh
